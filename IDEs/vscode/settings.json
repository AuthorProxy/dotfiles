    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "bash": {
      "path": "bash",
      "args": ["-l"],
      "icon": "terminal-bash"
    },
    "fish": {
      "path": "fish",
      "args": ["-l"]
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "quokka.startViewStatusBar": true,
  "quokka.compactMessageOutput": true,
  "quokka.automaticRestart": true,
  "quokka.suppressExpirationNotifications": true,
  "java.referencesCodeLens.enabled": true,
  "githubPullRequests.pullBranch": "never",
  "githubPullRequests.fileListLayout": "tree",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.typegen.ts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts, ${capture}.typegen.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
    "*.mts": "${capture}.typegen.ts",
    "*.cts": "${capture}.typegen.ts",
    ".eslintrc*": ".eslintrc*",
    "eslint.*": "eslint.*",
    ".prettierrc*": ".prettierrc*",
    "prettier.*": "prettier.*",
    "vue.config.*": "vite.config.*"
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "turboConsoleLog.addSemicolonInTheEnd": true,
  "turboConsoleLog.insertEmptyLineAfterLogMessage": true,
  "turboConsoleLog.quote": "'",
  "turboConsoleLog.wrapLogMessage": true,
