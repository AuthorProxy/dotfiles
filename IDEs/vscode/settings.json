  "workbench.editor.highlightModifiedTabs": true,
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true,
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.iconTheme": "catppuccin-frappe",
  "workbench.colorTheme": "Catppuccin Frapp√©",
  "workbench.productIconTheme": "feather-vscode",
  "workbench.colorCustomizations": {
    "activityBar.foreground": "#00f5f5",
    "activityBarBadge.foreground": "#0e2020",
    "activityBarBadge.background": "#333842",
    "editorLineNumber.foreground": "#f682e382",
    "editorCursor.foreground": "#00f5f5",
    "terminalCursor.background": "#7bf2b8",
    "terminalCursor.foreground": "#7bf2b8"
  },
  "theme-by-language.themes": {
    "*": "Catppuccin Frapp√©",
    "powershell": "PowerShell ISE"
  },
  "debug.allowBreakpointsEverywhere": true,
  "debug.showBreakpointsInOverviewRuler": true,
  "debug.showInlineBreakpointCandidates": true,
  "debug.javascript.breakOnConditionalError": true,
  "git.confirmForcePush": false,
  "git.confirmNoVerifyCommit": false,
  "explorer.confirmDelete": false,
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
