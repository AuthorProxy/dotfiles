{
  "editor.fontFamily": "FiraCode Nerd Font Mono, JetbrainsMono Nerd Font, monospace",
  "editor.fontSize": 14,
  "editor.fontWeight": "bold",
  "editor.fontLigatures": true,
  "editor.lineHeight": 26,
  "editor.tabSize": 2,
  "editor.rulers": [80, 100, 120],
  "editor.bracketPairColorization.enabled": true,
  "editor.language.colorizedBracketPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "true",
    "strings": "true"
  },
  "editor.detectIndentation": false,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 3000,
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "snippets": "json",
    "*.tpl": "html",
    "*.svg": "xml",
    "*.ext": "vue",
    "*.up": "sql",
    "*.down": "sql",
    ".prettierrc": "yaml"
  },
  "files.exclude": {
    "**/node_modules/**": true,
    "**/coverage/**": true
  },
  "better-comments.highlightPlainText": true,
  "editor.defaultFormatter": "prettier.prettier-vscode",
  "editor.formatOnType": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.organizeImports": "explicit",
    "source.fixAll": "explicit"
  },
  "formattingToggle.statusBarText": {
    "formattingEnabled": "4 8 15 16 23 42 $(gear~spin)",
    "formattingDisabled": "$(testing-failed-icon) $(testing-failed-icon) $(testing-failed-icon)",
    "formattingToggle.affects": ["editor.formatOnSave"]
  },
  "editor.acceptSuggestionOnEnter": "on",
  "editor.suggestSelection": "first",
  "editor.suggest.preview": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorWidth": 3,
  "editor.renderWhitespace": "all",
  "editor.letterSpacing": 0.5,
  "editor.minimap.enabled": true,
  "editor.minimap.scale": 3,
  "editor.minimap.autohide": "mouseover",
  "editor.minimap.maxColumn": 40,
  "editor.insertSpaces": true,
  "editor.linkedEditing": true,
  "editor.suggest.showMethods": true,
  "editor.suggest.preview": true,
  "editor.snippetSuggestions": "top",
  "files.participants.timeout": 0,
  "git.autofetch": "all",
  "git.allowForcePush": true,
  "git.allowNoVerifyCommit": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.followTagsWhenSync": true,
  "git.pruneOnFetch": true,
  "git.rebaseWhenSync": true,
  "git.showPushSuccessNotification": true,
  "git.supportCancellation": true,
  "git.autoStash": true,
  "git.useCommitInputAsStashMessage": true,
  "github.copilot.editor.enableCodeActions": false,
  "github.copilot.enable": {
    "*": true
  },
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "go.toolsManagement.autoUpdate": true,
  "go.enableCodeLens": {
    "runtest": true
  },
  "powershell.buttons.showPanelMovementButtons": true,
  "powershell.codeFormatting.autoCorrectAliases": true,
  "powershell.codeFormatting.trimWhitespaceAroundPipe": true,
  "powershell.codeFormatting.useCorrectCasing": true,
  "powershell.codeFormatting.whitespaceBetweenParameters": true,
  "powershell.codeFormatting.preset": "OTBS",
  "html.format.indentHandlebars": true,
  "html.format.templating": true,
  "html.format.preserveNewLines": false,
  // false to let stylelint rule validating
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "scss.format.spaceAroundSelectorSeparator": true,
  "npm.enableRunFromFolder": true,
  "js/ts.implicitProjectConfig.checkJs": true,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "javascript.referencesCodeLens.enabled": true,
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "javascript.preferences.quoteStyle": "single",
  "javascript.format.semicolons": "insert",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.validate.enable": true,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.format.semicolons": "insert",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescriptExplorer.typeTree.view.show.typeParameters": false,
  "typescriptExplorer.typeTree.view.show.baseClass": false,
  "cSpell.diagnosticLevel": "Hint",
  "emmet.includeLanguages": {
    "tpl": "html",
    "vue-html": "html",
    "javascript": "javascriptreact"
  },
  "[csharp]": {
    "editor.tabSize": 4
  },
  "csharp.semanticHighlighting.enabled": true,
